generator client {
  provider        = "prisma-client-js"
  output          = "../generated/prisma"
  previewFeatures = ["views"]
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

view FINTITPAGCLASFAZ {
  E2_CATEGFO String  @db.VarChar(1)
  E2_FORNECE String  @db.VarChar(6)
  E2_NOMFOR  String  @db.VarChar(20)
  E2_VALOR   Float
  MES        String? @db.VarChar(9)
  NUMERO     Int?
  E2_VENCREA String  @db.VarChar(8)
  E2_VENCTO  String  @db.VarChar(8)

  @@ignore
}

